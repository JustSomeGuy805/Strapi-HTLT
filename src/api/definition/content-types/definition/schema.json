      "required": true
    },
    "root": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::word.word"
    },
    "keywords": {
      "type": "string"
    },
    "related": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::word.word"
